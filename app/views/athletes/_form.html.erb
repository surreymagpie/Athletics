<%= form_for @athlete do |f| -%>
  <%= render 'layouts/errors', object: f.object %>
  <div class="field">
    <div class="small-3 columns"><%= f.label :first_name, "First Name", class: "inline" %></div>
    <div class="small-9 columns"><%= f.text_field :first_name %></div>
  </div>
  <div class="field">
    <div class="small-3 columns"><%= f.label :last_name, "Last Name", class: "inline" %></div>
    <div class="small-9 columns"><%= f.text_field :last_name %></div>
  </div>
  <div class="field">
    <div class="small-3 columns"><%= f.label :dob, "Date of birth", class: "inline" %></div>
    <div class="small-9 columns"><%= f.text_field :dob, class: "datepicker" %></div>
  </div>
  <div class="field">
    <div class="small-3 columns"><%= f.label :bib, "Race bib", class: "inline" %></div>
    <div class="small-9 columns"><%= f.text_field :bib %></div>
  </div>
  <div class="field">
    <div class="small-3 columns"><%= f.label :club_id, "Club", class: "inline"  %></div>
    <div class="small-9 columns"><%= f.collection_select(:club_id, Club.all, :id, :name) %></div>
  </div>
  <div class="field">
    <div class="small-3 columns"><%= f.label :second_claim, class: "inline" %></div>
    <div class="small-1 columns end"><%= f.check_box :second_claim %></div>
  </div>
  <div class="field">
     <div class="small-6 columns"><%= f.submit "Submit", :disable_with => 'Submiting...', class: "button large radius" %></div>
    <div class="small-6 columns"><%= link_to "Back", :back, class: "secondary button large radius right" %></div>
  </div>
<% end -%>